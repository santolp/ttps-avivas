<p style="color: green"><%= notice %></p>
<%= stylesheet_link_tag 'producto', media: 'all', data: { turbolinks: false } %> 

<div class="edit-product">
  
  <div class="product-show-btn">
  <%= link_to "Volver a lista de productos", productos_path%> 
  </div>
  <%= render @producto %>
  <div class="edit-buttons">
    <% if current_user %>
  <%= link_to "Editar producto", edit_producto_path(@producto) %> |
  
  <%= button_to "Eliminar producto", @producto, method: :delete %>
  <% end %>
</div>
</div>
